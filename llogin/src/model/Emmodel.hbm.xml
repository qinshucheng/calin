<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class name="model.Emmodel" table="patient">
    		<id name="empno" type="integer" column="empno">
    			<generator class="native">
  					<param name="sequence">emp_id</param>
    			</generator>
    		</id>
    		<property name="empname" type="string" column="empname"/>
    		<property name="empwd" type="string" column="empwd"/>
    		
    		<property name="sex" type="string" column="sex"/>
    		<property name="age" type="int" column="age"/>
    		<property name="post" type="string" column="post"/>
    		<property name="address" type="string" column="address"/>
    		<property name="detail" type="string" column="detail"/>
    	</class>
    
    </hibernate-mapping>